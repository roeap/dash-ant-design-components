{
  "name": "dash_ant_design_components",
  "version": "1.0.0",
  "description": "A Dash wrapper around React Ant Design Components",
  "main": "index.ts",
  "scripts": {
    "build:js::dev": "webpack --mode development",
    "build:js": "webpack",
    "build:backends": "dash-generate-components ./src/ts/components dash_ant_design_components -p package-info.json --r-prefix 'dadc' --jl-prefix 'dadc' --ignore \\.test\\.",
    "build": "npm run build:js && npm run build:backends",
    "watch": "npm run build:js::dev -- --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "css-loader": "^6.7.1",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react": "16.13.0",
    "react-docgen": "^5.4.0",
    "react-dom": "16.13.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^8.2.3",
    "typescript": "^4.6.2",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.9.1"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "author": "Robert Pack <robstar.pack@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.20.2",
    "ramda": "^0.28.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/ts/setupTests.js"
    ]
  }
}
